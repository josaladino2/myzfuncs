indent () {
	# Usage: [indentSize=2] [multiplier=1]
	if [ "${1}" = "-" ]; then
		if [ "${1}" = "-h" ]; then
			echo "$(fg_red1) "Usage"): ${funcstack[1]} [indentSize] [indent]"
			echo "Default: ${funcstack[1]} 2 1"
		fi
		shift
	else
  local indentSize=${1:-2}
  local indent=${2:-1}
  pr -to $((indentSize * indent))
}
