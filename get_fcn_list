get_fcn_list () {
	if [[ "${1}" =~ .*"-".* ]]; then
		if [ "${1}" = "-c" ]; then
			Cols="Y"
		fi
	shift
	fi
	NumCols=${1:-5}
	if [ "${Cols}" = "Y" ]; then
		awk '{$1=$1};1' ${myzfuncsfile}|egrep '^.* \(\) {'|egrep -v '^#'|awk '{ print $1 }'|sort|/usr/local/bin/rs 0 ${NumCols}
	else
		awk '{$1=$1};1' ${myzfuncsfile}|egrep '^.* \(\) {'|egrep -v '^#'|awk '{ print $1 }'|sort
	fi
}
